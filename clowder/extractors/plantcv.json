{
    "key": "plantcv",
    "label": "PlantCV extractor",
    "description": "Clowder extractor for PlantCV images",
    "image": "ndslabs/plantcv:0.9.2",
    "isStack": false,
    "isService": false,
    "isStandalone": true,
    "depends": [
        {
            "key": "rabbitmq",
            "required": true
        }
    ],
    "config":  [
        {
            "name": "RABBITMQ_EXCHANGE",
            "value": "clowder",
            "canOverride": false
        },
        {
            "name": "RABBITMQ_VHOST",
            "value": "%2F",
            "canOverride": false
        },
        {
            "name": "RABBITMQ_QUEUE",
            "value": "terra.plantcv",
            "canOverride": false
        }
    ]
}
