{
    "key": "imagepreview",
    "label": "Image Preview Extractor",
    "description": "Clowder extractor for image preview thumbnails",
    "image": {
		"name": "ndslabs/image-preview", 
		"tags" : ["0.9.2"]
	},
	"access": "internal",
    "depends": [
        {
            "key": "rabbitmq",
            "required": true
        }
    ],
    "config":  [
        {
            "name": "RABBITMQ_EXCHANGE",
            "value": "clowder",
            "canOverride": false
        },
        {
            "name": "RABBITMQ_VHOST",
            "value": "%2F",
            "canOverride": false
        },
        {
            "name": "RABBITMQ_QUEUE",
            "value": "ncsa.image.preview",
            "canOverride": false
        }
    ],
    "resourceLimits": {
        "cpuMax": "1",
        "cpuDefault": "100m",
        "memMax": "250Mi",
        "memDefault": "25Mi"
    }
}
