{
    "key": "openrefine",
    "label": "OpenRefine",
    "description": "A free, open source, power tool for working with messy data",
    "logo": "/asset/png/logos/openrefine-logo.png",
    "image": {
        "registry": "",
        "name": "craigwillis/openrefine",
        "tags": [
            "latest"
        ]
    },
    "display": "stack",
    "access": "external",
    "ports": [
        {
            "port": 8080,
            "protocol": "http",
            "contextPath": "/"
        }
    ],
    "readinessProbe": {
        "type": "",
        "path": "",
        "port": 0,
        "initialDelay": 0,
        "timeout": 0
    },
    "volumeMounts": [
        {
            "mountPath": "/home/researcher"
        }
    ],
    "resourceLimits": {
        "cpuMax": 500,
        "cpuDefault": 100,
        "memMax": 1000,
        "memDefault": 50
    },
    "authRequired": true
}
