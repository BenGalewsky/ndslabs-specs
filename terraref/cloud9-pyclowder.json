{
    "key": "cloud9pyclowder",
    "label": "Cloud9 Python environment for pyClowder",
    "image": {
		"name": "craigwillis/cloud9-pyclowder",
		"tags": ["latest"]
	}, 
    "display": "stack",
    "access": "external",
    "description": "Python development environment based on Cloud9 with pyClowder dependencies",
    "logo" : "/asset/png/logos/python-logo.png",
    "authRequired": true,
    "ports": [
        {
            "port": 80,
            "protocol": "http",
            "contextPath": "/"
        }
    ],
    "volumeMounts":[
       { "mountPath": "/workspace" }
    ],
    "readinessProbe" : {
        "type" : "http",
        "path" : "/favicon.ico",
        "port" : 80,
        "initialDelay": 10,
        "timeout" : 360
    },
    "resourceLimits": {
        "cpuMax": 1000,
        "cpuDefault": 100,
        "memMax": 1000,
        "memDefault": 50
    },
	"info" : "https://nationaldataservice.atlassian.net/wiki/display/NDSC/Cloud9",
	"tags" : ["7", "20"]
}
