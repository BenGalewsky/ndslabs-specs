{
    "key": "ontowiki",
    "label": "OntoWiki",
    "description": "Semantic wiki and knowledgebase",
    "image": {
        "name": "aksw/ontowiki-ubuntu-nginx",
        "tags": [
            "latest"
        ]
    },
    "display": "stack",
    "access": "external",
    "ports": [
        {
            "port": 80,
            "protocol": "http"
        }
    ],
    "repositories": [
		{
			"type":"git",
			"url":"https://github.com/AKSW/OntoWiki/wiki"
		}
	],
    "readinessProbe": {
        "type": "",
        "path": "",
        "port": 0,
        "initialDelay": 0,
        "timeout": 0
    },
    "volumeMounts": [],
    "resourceLimits": {
        "cpuMax": 200,
        "cpuDefault": 100,
        "memMax": 512,
        "memDefault": 50
    },
    "tags": ["32"]
}
