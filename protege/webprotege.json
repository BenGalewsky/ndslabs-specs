{
    "key": "webprotege",
    "label": "WebProtégé",
    "description": "Ontology development environment",
    "logo": "http://protege.stanford.edu/download/protege/4.3/installanywhere/Web_Installers/InstData/com/zerog/ia/installer/images/Splash.gif",
    "image": {
        "name": "ndslabs/webprotege",
        "tags": [
            "latest"
        ]
    },
    "display": "stack",
    "access": "external",
    "depends": [
        {
            "key": "mongo",
            "required": true,
            "shareConfig": false
        }
    ],
    "ports": [
        {
            "port": 8080,
            "protocol": "http"
        }
    ],
    "repositories": null,
    "readinessProbe": {
        "type": "http",
        "path": "/images/bp_icon.png",
        "port": 8080,
        "initialDelay": 30,
        "timeout": 500
    },
    "resourceLimits": {
        "cpuMax": 200,
        "cpuDefault": 100,
        "memMax": 1000,
        "memDefault": 50
    },
    "tags": ["32"]
}
