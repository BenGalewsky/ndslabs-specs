{
  "key": "orront3",
  "label": "Ontology Registry and Repository",
  "description": "Registry for publishing and managing ontologies",
  "logo": "https://avatars0.githubusercontent.com/u/4140922?v=3&s=400",
  "image": {
    "name": "ndslabs/orront",
    "tags": [
      "latest"
    ]
  },
  "display": "stack",
  "access": "external",
  "depends": [
    {
      "key": "mongo",
      "required": true,
      "shareConfig": false
    },
    {
      "key": "agraph",
      "required": true,
      "shareConfig": false
    }
  ],
  "config": [
    {
      "name": "AGRAPH_PORT",
      "value": "10035",
      "label": "Agraph Port",
      "isPassword": false,
      "canOverride": true
    },
    {
      "name": "MONGO_PORT",
      "value": "27017",
      "label": "Mongo Port",
      "isPassword": false,
      "canOverride": true
    },
    {
      "name": "ADMIN_PASSWORD",
      "value": "",
      "label": "Admin Password",
      "isPassword": true,
      "canOverride": false
    },
    {
      "name": "ADMIN_EMAIL",
      "value": "",
      "label": "Admin email",
      "isPassword": false,
      "canOverride": true
    },
    {
      "name": "MONGO_DB_NAME",
      "value": "orr-ont",
      "label": "Mongo DB Name",
      "isPassword": false,
      "canOverride": true
    },
    {
      "name": "AGRAPH_USERNAME",
      "value": "test",
      "label": "Agraph Username",
      "isPassword": false,
      "canOverride": true
    },
    {
      "name": "AGRAPH_PASSWORD",
      "value": "xyzzy",
      "label": "Agraph password",
      "isPassword": true,
      "canOverride": true
    },
    {
      "name": "AGRAPH_REPO_NAME",
      "value": "mmiorr",
      "label": "Agraph Repo Name",
      "isPassword": false,
      "canOverride": true
    },
    {
      "name": "AGRAPH_HOST",
      "value": "$(AGRAPH_PORT_10035_TCP_ADDR)",
      "label": "Agraph Host",
      "isPassword": false,
      "canOverride": false
    },
    {
      "name": "MONGO_HOST",
      "value": "$(MONGO_PORT_27017_TCP_ADDR)",
      "label": "Mongo Host",
      "isPassword": false,
      "canOverride": false
    }
  ],
  "command": null,
  "args": null,
  "ports": [
    {
      "port": 8080,
      "protocol": "http"
    }
  ],
  "repositories": [
    {
      "url": "https://github.com/mmisw/orr-ont",
      "type": "git"
    }
  ],
  "readinessProbe": {
    "type": "",
    "path": "",
    "port": 0,
    "initialDelay": 0,
    "timeout": 0
  },
  "resourceLimits": {
    "cpuMax": 500,
    "cpuDefault": 100,
    "memMax": 1000,
    "memDefault": 50
  },
  "tags": [
    "32"
  ],
}
