{
    "key": "cloudbrowserui",
    "label": "iRODS Cloudbrowser UI",
    "requiresVolume": false,
    "image": "ndslabs/cloudbrowser-ui:latest",
    "isService": true,
    "isStandalone": true,
    "isStack": false,
    "isPublic": true,
    "ports": [
        {
            "port": 80,
            "protocol": "http"
        }
    ],
    "depends": [
      { "key": "cloudbrowser", "required": true }
    ],
    "readinessProbe" : {
        "type" : "http",
        "path" : "app/images/beta_test_logo.png",
        "port" : 80,
        "initialDelay": 30,
        "timeout" : 600
    }
}
