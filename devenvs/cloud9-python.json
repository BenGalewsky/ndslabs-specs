{
    "key": "cloud9py",
    "label": "Cloud9 Python",
    "image": {
		"name": "ndslabs/cloud9-python",
		"tags": ["latest"]
	}, 
    "display": "stack",
    "access": "external",
    "description": "Python development environment based on Cloud9",
    "logo" : "/asset/png/logos/python-logo.png",
    "authRequired": true,
    "ports": [
        {
            "port": 80,
            "protocol": "http",
            "contextPath": "/"
        },
        {
            "port": 8080,
            "protocol": "http",
            "contextPath": "/"
        }
    ],
    "volumeMounts":[
       { "mountPath": "/workspace","defaultPath":"/" }
    ],
    "readinessProbe" : {
        "type" : "http",
        "path" : "/favicon.ico",
        "port" : 80,
        "initialDelay": 10,
        "timeout" : 360
    },
    "resourceLimits": {
        "cpuMax": 2000,
        "cpuDefault": 100,
        "memMax": 4000,
        "memDefault": 50
    },
	"info" : "https://nationaldataservice.atlassian.net/wiki/display/NDSC/Cloud9",
	"tags" : ["7", "20"]
}
