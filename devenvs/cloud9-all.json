{
    "key": "cloud9all",
    "label": "Cloud9",
    "image": {
		"name": "ndslabs/cloud9-all",
		"tags": ["latest"]
	}, 
	"logo" : "/asset/png/logos/cloud9-logo.png",
    "display": "stack",
    "access": "external",
    "description": "Multiple-language development environment (Java, NodeJS, PHP, Python, Rails)",
    "ports": [
        {
            "port": 80,
            "protocol": "http",
            "contextPath": "/"
        },
        {
            "port": 8080,
            "protocol": "http",
            "contextPath": "/"
        }
    ],
    "volumeMounts":[
       { "mountPath": "/workspace","defaultPath":"/" }
    ],
    "readinessProbe" : {
        "type" : "http",
        "path" : "/favicon.ico",
        "port" : 80,
        "initialDelay": 10,
        "timeout" : 600
    },
    "resourceLimits": {
        "cpuMax": 2000,
        "cpuDefault": 100,
        "memMax": 4000,
        "memDefault": 50
    },
	"info": "https://nationaldataservice.atlassian.net/wiki/display/NDSC/Cloud9",
	"authRequired": true,
	"tags" : ["7", "16"]
}
