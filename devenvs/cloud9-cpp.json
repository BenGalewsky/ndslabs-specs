{
    "key": "cloud9cpp",
    "label": "Cloud9 C/C++",
    "image": "ndslabs/cloud9-cpp",
    "display": "stack",
    "access": "external",
    "description": "Unopinionated C/C++ development environment based on Cloud9",
    "ports": [
        {
            "port": 80,
            "protocol": "http"
        }
    ],
    "volumeMounts":[
       { "mountPath": "/workspace", "name": "cloud9cpp" }
    ],
    "readinessProbe" : {
        "type" : "http",
        "path" : "/favicon.ico",
        "port" : 80,
        "initialDelay": 10,
        "timeout" : 600
    },
    "resourceLimits": {
        "cpuMax": "1",
        "cpuDefault": "100m",
        "memMax": "1Gi",
        "memDefault": "50Mi"
    }
}
