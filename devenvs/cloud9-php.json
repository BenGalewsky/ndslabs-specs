{
    "key": "cloud9php",
    "label": "Cloud9 PHP",
    "image": {
		"name": "ndslabs/cloud9-php",
		"tags": ["latest", "v1.0.2"]
	}, 
	"logo" : "/asset/png/logos/php-logo.png",
    "display": "stack",
    "access": "external",
    "description": "PHP development environment based on Cloud9",
    "ports": [
        {
            "port": 80,
            "protocol": "http",
            "contextPath": "/"
        },
        {
            "port": 8080,
            "protocol": "http",
            "contextPath": "/"
        }
    ],
    "volumeMounts":[
       { "mountPath": "/workspace","defaultPath":"/" }
    ],
    "readinessProbe" : {
        "type" : "http",
        "path" : "/favicon.ico",
        "port" : 80,
        "initialDelay": 10,
        "timeout" : 360
    },
    "resourceLimits": {
        "cpuMax": 2000,
        "cpuDefault": 100,
        "memMax": 4000,
        "memDefault": 50
    },
	"info": "https://nationaldataservice.atlassian.net/wiki/display/NDSC/Cloud9",
	"authRequired": true,
	"tags" : ["7", "18"]
}
