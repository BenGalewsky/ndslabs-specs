{
    "key": "rserve",
	"access": "internal",
	"display": "standalone",
    "label": "Rserve",
    "image": {
		"name": "ndslabs/dataverse-rserve",
		"tags": ["latest"]
	},
    "ports": [
        { "port": 6311, "protocol": "tcp" }
    ],
    "resourceLimits": {
        "cpuMax": "1",
        "cpuDefault": "100m",
        "memMax": "250Mi",
        "memDefault": "100Mi"
    }
}
