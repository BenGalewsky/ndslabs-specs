{
    "key": "elasticsearch2",
    "label": "ElasticSearch",
    "image": "elasticsearch:2.2",
    "description": "A distributed search and analytics engine",
	"display": "standalone",
	"access":"internal",
    "ports": [
        { "port": 9200, "protocol": "tcp" }
    ],
    "volumeMounts":[
        { "mountPath": "/usr/share/elasticsearch/data", "name": "elasticsearch2" }
    ],
    "resourceLimits": {
        "cpuMax": "1",
        "cpuDefault": "100m",
        "memMax": "500Mi",
        "memDefault": "100Mi"
    }
}
